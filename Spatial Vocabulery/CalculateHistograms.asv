function [ histograms ] = CalculateHistograms( landmarksDB, origin, Q)
% landmarksDB n x l x 2 (n - number of images, l - number of chosen landmarks)
% OUTPUT : histograms is a k x Q x 2

[n,l,~] = size(landmarksDB);

% Call CalculateHistogram on each landmark
for i=1:n
    currLandmarkHistogram = CalculateHistogram(,origin,Q);
    currLandmarkHistogram
end

end

